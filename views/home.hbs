<script>

    new Vue({
        el: '#hp-search',
        created: function () {
            this.loadRecents();
            this.loadFavorites();
        },
        data: function () {
            return {
                searchQuery: '',
                searchTimeout: null,
                searchAbort: null,
                searchResults: null,
                searchResultsVisible: false,
                isAndroid: false,
                isIos: false,
                activeTabInt: null,
                recents: [],
                favorites: [],
                popular: [
                    { console: null, name: 'bughа' },
                    { console: null, name: 'Tfue' },
                    { console: null, name: 'FaZe Mongraal' },
                    { console: null, name: 'NRG benjyfishy' },
                    { console: null, name: '100T MrSavage' },
                    { console: null, name: 'MSF Clix' },
                    { console: null, name: 'NRG Unknown.' },
                    { console: null, name: 'EatDatAydan445.' },
                    { console: null, name: 'NRG Zayt -α-' },
                    { console: null, name: 'TSM KHANADA cc' }
                ]
            }
        },
        computed: {
            activeTab: {
                get: function () {
                    if (this.activeTabInt == null) {
                        this.activeTabInt = localStorage.getItem('hp_search_tab');
                        if (this.activeTabInt == null)
                            this.activeTabInt = 'recent';
                    }
                    return this.activeTabInt;
                },
                set: function (value) {
                    localStorage.setItem('hp_search_tab', value);
                    this.activeTabInt = value;
                },
                cache: false
            },
            recentsList: function () {
                var self = this;
                return self.recents.map(function (value) {
                    value.name = self.htmlDecode(value.name);
                   // console.log(value)
                    value.platform = self.getPlatformShortName(value.console);
                    value.url = '/profile/' + value.platform + '/' + value.name;

                    return value;
                });
            },
            favoritesList: function () {
                var self = this;
                return self.favorites.map(function (value) {
                    value.name = self.htmlDecode(value.name);
                    value.platform = self.getPlatformShortName(value.console);
                    value.url = '/profile/' + value.platform + '/' + value.name;

                    return value;
                })
            },
            popularList: function () {
                var self = this;
                return self.popular.map(function (value) {
                    value.name = self.htmlDecode(value.name);
                    value.platform = self.getPlatformShortName(value.console);
                    value.url = '/profile/' + value.platform + '/' + value.name;

                    return value;
                })
            }            
        },
        methods: {
            getPlatformShortName(platformId) {
                switch (platformId) {
                    case 1:
                        return 'xbox';
                    case 2:
                        return 'psn';
                    case 101:
                        return 'gamepad';
                    case 3:
                        return 'pc';
                    case 103:
                        return 'kbm';
                    case 104:
                        return 'touch';
                    default:
                        return 'all';
                }
            },
            showSearchResults() {
                this.searchResultsVisible = true
            },
            hideSearchResults() {
                this.searchResultsVisible = false
            },
            loadRecents() {
                var recent = localStorage.getItem('recentv3') || '';
                this.recents = recent !== '' ? JSON.parse(recent) : [];
            },
            saveRecents() {
                localStorage.setItem('recentv3', JSON.stringify(this.recents.map(function (value) {
                    return {
                        name: value.name,
                        console: value.console
                    }
                })));
            },
            loadFavorites() {
                var favorite = localStorage.getItem('favoritev3') || '';
                var arr = favorite !== '' ? JSON.parse(favorite) : [];
                this.favorites = arr;
            },
            saveFavorites() {
                localStorage.setItem('favoritev3', JSON.stringify(this.favorites));
            },

            getPopular() {
                return []
            },
             toggleFavorite(player) {
                var firstFav = this.favorites.findIndex(function (val) {
                    return val.name == player.name
                });
                
                if (firstFav != -1) {
                    this.favorites.splice(firstFav, 1);
                } else {
                    this.favorites.unshift(player);
                }
                this.saveFavorites();
            },
            isFavorite(player) {
                return this.favorites.filter(function (val) {
                    return val.name == player.name;
                }).length > 0;
            },
            removeRecent(player) {
                this.recents = this.recents.filter(function (val) {
                    return val.name != player.name
                });
                this.saveRecents();
            },
            htmlDecode(str) {
                return str.replace(/&#(\d+);/g, function (match, dec) {
                    return String.fromCharCode(dec);
                });
            }
        },
        watch: {
            'searchQuery': function (newValue, oldValue) {
                if (!window.fetch) {
                    return;
                }

                if (!this.searchAbort) {
                    this.searchAbort = new AbortController();
                }

                if (newValue == oldValue) {
                    return;
                }

                this.searchAbort.abort();
                this.searchAbort = new AbortController();
                clearTimeout(this.searchTimeout)

                if (newValue == '' || newValue.length < 2) {
                    this.searchResults = null;
                    this.activeTabInt = null;
                    return;
                }

                this.searchTimeout = setTimeout(() => {
                    fetch('https://search-api.tracker.network/search/fortnite?advanced=1&q=' + newValue, { signal: this.searchAbort.signal })
                    .then((response) => {
                        return response.json()
                    })
                    .then((data) => {
                        this.activeTabInt = 'results';
                        this.searchResults = data
                    })
                        .catch(ex => {})
                }, 300);
            }
        },
        directives: {
            'click-outside': {
                bind: function (el, binding, vNode) {
                    // Provided expression must evaluate to a function.
                    if (typeof binding.value !== 'function') {
                        const compName = vNode.context.name
                        let warn = '[Vue-click-outside:] provided expression ' + binding.expression + ' is not a function, but has to be'
                        if (compName) { warn += 'Found in component ' + compName }

                        console.warn(warn)
                    }
                    // Define Handler and cache it on the element
                    const bubble = binding.modifiers.bubble
                    const handler = (e) => {
                        if (bubble || (!el.contains(e.target) && el !== e.target)) {
                            binding.value(e)
                        }
                    }
                    el.__vueClickOutside__ = handler

                    // add Event Listeners
                    document.addEventListener('click', handler)
                },

                unbind: function (el, binding) {
                    // Remove Event Listeners
                    document.removeEventListener('click', el.__vueClickOutside__)
                    el.__vueClickOutside__ = null

                }
            }
        }
    });

</script>
