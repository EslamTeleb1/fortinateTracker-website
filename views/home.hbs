        data: function () {
            return {
                userSettingsCollapsed: true,
                moreMenuCollapsed: true,
                searchShown: false,
                addProfileShown: false
            }
        },
        computed: {
            isUserSettingsCollapsed: function () {
                return this.userSettingsCollapsed
            }
        },
        methods: {
            toggleMoreMenu: function () {
                this.moreMenuCollapsed = !this.moreMenuCollapsed
            },
            toggleUserSettings: function () {
                this.userSettingsCollapsed = !this.userSettingsCollapsed
            },
            toggleSearch() {
                this.addProfileShown = false
                this.searchShown = !this.searchShown
            },
            onSearchClose: function () {
                this.searchShown = false;
            },
            onSearchPlayerChosen: function (player) {
                location.href = '/profile/search?q=' + player.platformUserHandle;
            }
        }
    })
