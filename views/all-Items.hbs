<script type="text/javascript">
    Vue.component('search-box', {
        template: '#search-box-template',
        data: function () {
            return {
                currentPlatform: 3,
                currentGroupKey: 'recents',
                searchInput: '',
                historyShown: false,
                searchResultsLoading: false,
                searchResults: null,
                favorites: [],
                recents: [],
            }
        },
        mounted: function () {
            var recent = localStorage.getItem('recentv3') || '';
            var recents = recent !== '' ? JSON.parse(recent) : [];
            this.recents = recents.map(function (player) {
                return {
                    platformUserHandle: player.name,
                    platformId: player.console
                }
            });

            var favorite = localStorage.getItem('favoritev3') || '';
            var favorites = favorite !== '' ? JSON.parse(favorite) : [];
            this.favorites = favorites.map(function (player) {
                return {
                    platformUserHandle: player.name,
                    platformId: player.console
                }
            });

            this.$refs.input.focus();
        },
        computed: {
            players: function () {
                if (this.currentGroupKey === 'recents') {
                    return this.recents
                } else if (this.currentGroupKey === 'favorites') {
                    return this.favorites
                } else {
                    return []
                }
            }
        },
        methods: {
            toggleGroupMembership: function (groupKey, player) {
                if (this.isInGroup(groupKey, player)) {
                    if (groupKey === 'favorites') {
                        this.favorites = this.favorites.filter(function (p) {
                            return p.platformUserHandle !== player.platformUserHandle
                        })
                        localStorage.setItem('favoritev3', JSON.stringify(this.favorites));
                    } else if (groupKey == 'recents') {
                        this.recents = this.recents.filter(function (p) {
                            return p.platformUserHandle !== player.platformUserHandle
                        })
                        localStorage.setItem('recentv3', JSON.stringify(this.recents));
                    }
                } else {
                    if (groupKey === 'favorites') {
                        this.favorites = this.favorites.filter(function (p) {
                            return p.platformUserHandle !== player.platformUserHandle
                        })
                        this.favorites.push(player)
                        localStorage.setItem('favoritev3', JSON.stringify(this.favorites.map(function (p) {
                            return {
                                console: p.platformId,
                                name: p.platformUserHandle
                            };
                        })));
                    }
                }
            },
            isInGroup: function (groupKey, player) {
                if (groupKey == 'recents') {
                    return this.recents.find(function (p) {
                        return p.platformUserHandle === player.platformUserHandle
                    }) != null;
                } else if (this.currentGroupKey === 'favorites') {
                    return this.favorites.find(function (p) {
                        return p.platformUserHandle === player.platformUserHandle
                    }) != null;
                }
            },
            chooseProfile: function (player) {
                this.$emit('player-chosen', player)
            },
            search: function () {
                this.$emit('player-chosen', {
                    platformId: this.currentPlatform,
                    platformUserHandle: this.searchInput
                });
            }
        }
    });
</script>
